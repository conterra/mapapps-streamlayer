{
  "properties": {
    "title": "Bus-Tracker",
    "amdPackages": "apprt@^3.2.1"
  },
  "load": {
    "bundleLocations": [
      "localbundles",
      "bundles"
    ],
    "allowedBundles": [
      "notifier@^3.2.1",
      "system@^3.2.1",
      "splashscreen@^3.2.1",
      "map@^3.2.1",
      "themes@^3.2.1",
      "templates@^3.2.1",
      "templatelayout@^3.2.1",
      "windowmanager@^3.2.1",
      "toolset@^3.2.1",
      "toolrules@^3.2.1",
      "basemaptoggler@^3.2.1",
      "banner@^3.2.1",
      "streamlayerservice",
      "bustracker"
    ],
    "require": [
      "${app}.app"
    ],
    "i18n": [
      "bundle"
    ]
  },
  "bundles": {
    "banner": {
      "BannerWidget": {
        "label": "Developer Network",
        "image": "https://www.conterra.de/~/media/conterra/images/site_logos/conterra-logo.ashx",
        "imagePosition": "above",
        "link": "http://developernetwork.conterra.de"
      }
    },
    "map": {
      "MapState": {
        "initialExtent": {
          "xmin": -13652648.195859605,
          "xmax": -13585956.888680877,
          "ymin": 6022016.456696275,
          "ymax": 6056948.178622554,
          "wkid": 3857
        }
      },
      "MapModelFactory": {
        "_configData": {
          "maps": [
            {
              "id": "default",
              "glass_pane": [
                {
                  "id": "bbox",
                  "enabled": true,
                  "graphics": []
                }
              ],
              "operationalLayer": [
                {
                  "title": "Seattle Bus Service",
                  "enabled": true,
                  "service": "seattle_busservice",
                  "wkid": "3857",
                  "id": "seattle_busservice",
                  "category": {
                    "title": "Seattle Bus Service",
                    "description": "Buses in Seattle"
                  },
                  "displayCount": "100",
                  "infoTemplate": {
                    "title": "${BusNo}",
                    "content": "Bus No: ${BusNo}<br>Heading: ${Heading}Â°<br>RouteID: ${RouteID}<br>"
                  },
                  "symbol": {
                    "type": "esriPMS",
                    "url": "resource('${app}:images/bus.png')",
                    "contentType": "image/png",
                    "color": null,
                    "width": 16,
                    "height": 16,
                    "angle": 0,
                    "xoffset": 0,
                    "yoffset": 0
                  }
                }
              ],
              "baseLayer": [
                {
                  "title": "${service.streets}",
                  "service": "streets",
                  "enabled": false
                },
                {
                  "title": "${service.topo}",
                  "service": "worldTopo",
                  "enabled": true
                }
              ]
            }
          ]
        }
      },
      "MappingResourceRegistryFactory": {
        "_knownServices": {
          "services": [
            {
              "id": "seattle_busservice",
              "url": "wss://geoeventsample3.esri.com:8443/arcgis/ws/services/SeattleBus/StreamServer/subscribe",
              "type": "STREAM_LAYER",
              "title": "Seattle Bus Service",
              "description": "Buses in Seattle"
            },
            {
              "id": "worldTopo",
              "type": "AGS_TILED",
              "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer"
            },
            {
              "id": "streets",
              "type": "AGS_TILED",
              "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
            }
          ]
        }
      }
    }
  }
}